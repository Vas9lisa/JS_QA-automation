let skobki = [")", "("]
let pravo = 0
let levo = 0
for(let i = 0; i < skobki.length; i++) {
  if(skobki[i] == "("){
    levo++
  } else {
    pravo++
  }
}
if(levo == pravo){
  console.log("–£ –∫–∞–∂–¥–æ–π —Å–∫–æ–±–∫–∏ –µ—Å—Ç—å —Å–≤–æ—è –ø–∞—Ä–∞ <3")
} else {
  console.log("–ï—Å—Ç—å –µ—â–µ —Å–∫–æ–±–∫–∏ –±–µ–∑ —Å–≤–æ–µ–π –ø–æ–ª–æ–≤–∏–Ω–∫–∏")
