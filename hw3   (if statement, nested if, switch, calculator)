let a = 12
let b = 2 
let sign = "&"

switch(sign){
  case "+" :
       document.write (a+b)
    break
  case "-" :
       document.write (a-b)
    break
  case "*" :
       document.write (a*b)
    break
  case "/" :
       document.write (a/b)
    break 
  default :
       document.write ("unknown value")
}
